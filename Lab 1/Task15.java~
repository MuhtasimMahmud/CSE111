import java.util.Scanner;
public class Task15
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("Please give me a number");
    int n=sc.nextInt();
    for(int i=1;i<=n;i++)
    {
      System.out.print(i);
    }
    System.out.println();
    sc.close();
  }
}